{
    "name": "robwittman/php-shhopify-app",
    "description": "Template Shopify App using PHP Slim API, React - Redux, and Docker",
    "authors": [
        {
            "name": "Robert Wittman",
            "email": "robert.wittman@webpt.com"
        }
    ],
    "require": {
        "slim/slim": "^3.10",
        "tuupola/slim-jwt-auth": "^3.0",
        "flexsounds/slim-symfony-di-container": "^1.0",
        "doctrine/dbal": "^2.7",
        "bernard/bernard": "^0.12.4",
        "firebase/php-jwt": "^5.0",
        "symfony/console": "^4.0",
        "predis/predis": "^1.1",
        "league/flysystem": "^1.0",
        "aws/aws-sdk-php": "^3.55",
        "symfony/config": "^4.0",
        "robwittman/shopify-php-sdk": "^2.1",
        "monolog/monolog": "^1.23",
        "psr/cache": "^1.0",
        "robmorgan/phinx": "^0.9.2",
        "symfony/event-dispatcher": "^4.0",
        "javibravo/simpleue": "^2.1",
        "doctrine/orm": "^2.6",
        "pusher/pusher-php-server": "^3.0",
        "tuupola/cors-middleware": "^0.7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "fzaninotto/faker": "^1.7"
    }
}
